#!/usr/bin/env bash

function _cue_profile {
  local shome="${_cue_home:-"$(cd -P -- "$(dirname -- "$BASH_SOURCE")/.." && pwd -P)"}"
  PATH="$shome/exec:$PATH"

  source "cue_solarized"
  source "cue_pwd"
  source "cue_ssh"
  source "cue_git"
  source "cue_theme"
  source "cue_prompt"
}

_cue_profile
